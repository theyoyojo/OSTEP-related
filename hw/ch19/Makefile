CC 	= gcc
CFLAGS  = -g -std=gnu11 -O0
OBJECTS = main.o ez_tsc.o
BIN	= tlbmeasure

all: $(OBJECTS)
	$(CC) $(CFLAGS) $(OBJECTS) -o $(BIN)

%.o: %.c
	$(CC) $(CFLAGS) -c $^ -o $@

.PHONEY: clean
clean:
	rm -rf $(BIN) $(OBJECTS)

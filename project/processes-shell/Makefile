CC 	= gcc
CFLAGS  = -g -std=gnu11
OBJECTS = main.o wish_context.o wish_input.o wish_builtin.o wish_parse.o
BIN	= wish

all: $(OBJECTS)
	$(CC) $(CFLAGS) $(OBJECTS) -o $(BIN)

%.o: %.c
	$(CC) $(CFLAGS) -c $^ -o $@

.PHONEY: clean
clean:
	rm -rf $(BIN) $(OBJECTS)
